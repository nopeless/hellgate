{
  "name": "hellgate",
  "version": "2.0.1-beta",
  "description": "An agonistic access control library with great flexibility",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "mocha",
    "test:coverage": "c8 mocha",
    "build": "rimraf dist && tsc",
    "lint": "eslint src test --ext ts --fix",
    "locov": "c8 --reporter=lcov npm test",
    "test:html": "c8 --reporter=html npm run test && echo 'Opening index.html...' && (start coverage/index.html || echo 'could not open coverage/index.html')"
  },
  "keywords": [
    "rbac",
    "ts",
    "typescript",
    "deny",
    "customizable",
    "flexible",
    "permission"
  ],
  "author": "nopeless",
  "repository": {
    "url": "https://github.com/nopeless/hellgate"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "c8": "^7.12.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-quotes": "^0.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^4.1.2",
    "tsafe": "1.2.1",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {}
}
